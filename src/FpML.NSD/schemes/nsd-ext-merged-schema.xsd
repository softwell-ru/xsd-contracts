            <xsd:documentation source="nsdr" xml:lang="ru"/>
            <xsd:documentation source="nsdr" xml:lang="en"/>
            <xsd:documentation source="ffms" xml:lang="ru"/>
            <xsd:documentation source="ffms" xml:lang="en"/>
