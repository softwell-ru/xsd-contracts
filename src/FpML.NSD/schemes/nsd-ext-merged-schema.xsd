        <xsd:sequence>
            <xsd:element name="partyReference" type="fpml:PartyReference"/>
            <xsd:element name="relatedParty"
                      type="fpml:RelatedParty"
                      minOccurs="0"
                      maxOccurs="unbounded"/>
