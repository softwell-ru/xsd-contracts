						            <xsd:appinfo source="sfr"/>
						            <xsd:documentation source="nsdr" xml:lang="ru">Договор заключен не в интересах клиента</xsd:documentation>
						            <xsd:documentation source="nsdr" xml:lang="en">Own party trade</xsd:documentation>
