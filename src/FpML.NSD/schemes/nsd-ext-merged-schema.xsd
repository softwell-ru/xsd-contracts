                    <xsd:documentation source="nsdr" xml:lang="en">Undefined</xsd:documentation>
                    <xsd:documentation source="ffms" xml:lang="ru"/>
                    <xsd:documentation source="ffms" xml:lang="en"/>
                </xsd:annotation>                
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
    
