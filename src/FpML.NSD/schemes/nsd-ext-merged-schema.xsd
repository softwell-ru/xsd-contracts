					          <xsd:documentation source="stdc" xml:lang="ru">Клиент является иным юридическим лицом</xsd:documentation>
					          <xsd:documentation source="stdc" xml:lang="en"/>
				        </xsd:annotation>
			      </xsd:enumeration>
