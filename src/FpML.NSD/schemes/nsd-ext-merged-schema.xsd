						            <xsd:documentation source="nsdr" xml:lang="en">An identifier of the Master Agreement</xsd:documentation>
					          </xsd:annotation>
				        </xsd:element>
				        <xsd:element name="counterParty" type="fpml:Party" maxOccurs="unbounded">
