<xsd:group ref="FxExpiryDateOrSchedule.model"/>
<xsd:group ref="FxSettlementDateOrSchedule.model"/>
<xsd:element name="barrier" type="FxAccrualBarrier" minOccurs="0" maxOccurs="unbounded"/>
