<xsd:sequence>
<xsd:group ref="ParametricSchedule.model"/>
<xsd:choice minOccurs="0">
<xsd:element name="numberOfFixings" type="xsd:nonNegativeInteger"/>
<xsd:element name="fixingDate" type="xsd:date" maxOccurs="unbounded"/>
