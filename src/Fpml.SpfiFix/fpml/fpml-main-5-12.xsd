</xsd:sequence>
<xsd:sequence>
<xsd:group ref="ParametricSchedule.model"/>
<xsd:choice minOccurs="0">
<xsd:element name="numberOfFixings" type="xsd:nonNegativeInteger"/>
<xsd:element name="fixing" type="FxFixingObservation" maxOccurs="unbounded"/>
</xsd:choice>
</xsd:sequence>
</xsd:choice>
</xsd:complexType>
