</xsd:complexType>
<xsd:complexType name="FxFixingSchedule">
<xsd:choice>
<xsd:sequence>
<xsd:element name="dateAdjustments" type="BusinessDayAdjustments" minOccurs="0"/>
<xsd:element name="fixingDate" type="xsd:date" maxOccurs="unbounded"/>
